{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 115"
  ]
}